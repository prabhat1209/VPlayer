<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vplayer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/micon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/micon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PlayBookmarkedVideoActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="fullSensor"></activity>
        <activity
            android:name=".PlayVideoActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="fullSensor"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".bookmarked.BookmarkedActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>